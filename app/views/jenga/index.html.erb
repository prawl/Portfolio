<div id="jenga"> </div>
<div class="project-container">
  <div class="project-media">
    <div class="project-media-container">
      <div class="project-img">
        <a href="img/jenga_2.png" data-lightbox="jenga" title="Main Menu">
          <%= image_tag("jenga_2.png", alt: "Jenga Image 1") %>
        </a>
      </div>
      <div class="project-img">
        <a href="img/jenga_3.png" data-lightbox="jenga" title="Developers Stage">
          <%= image_tag("jenga_3.png", alt: "Jenga Image 2") %>
        </a>
      </div>
      <div class="project-img">
